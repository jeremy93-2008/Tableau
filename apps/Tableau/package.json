    "@fontsource/raleway": "^4.5.11",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/bundle-analyzer": "^13.1.6",
    "@next/font": "13.1.1",
    "@prisma/client": "^4.8.0",
    "@tanstack/react-query": "^4.22.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "aws-sdk": "^2.1317.0",
    "axios": "^1.6.0",
    "chakra-react-select": "^4.4.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "jotai": "^1.13.1",
    "next": "13.5.6",
    "next-auth": "^4.19.2",
    "rdndmb-html5-to-touch": "^8.0.0",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
