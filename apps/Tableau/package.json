{
  "name": "tableau",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "start:docker": "docker compose up",
    "prisma:studio": "dotenv -e .env -- npx prisma studio",
    "prisma:generate": "dotenv -e .env -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "prisma:reset": "dotenv -e .env -- npx prisma db push --force-reset ",
    "prisma:seed": "dotenv -e .env -- npx prisma db seed",
    "vercel-build": "npm run prisma:generate && next build",
    "e2e": "docker compose up -d && npx prisma migrate reset --skip-seed --skip-generate --force && cypress run"
  },
  "prisma": {
    "seed": "npx tsx prisma/seeder/seed.mts"
  },
  "dependencies": {
    "@azure/communication-email": "^1.0.0-beta.1",
    "@chakra-ui/icons": "^2.0.16",
